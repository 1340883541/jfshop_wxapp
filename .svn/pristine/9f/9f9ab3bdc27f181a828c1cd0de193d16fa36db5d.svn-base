/*公共css  */

.chunk-section {
    padding: 0 15px;
}

.chunk-section-main {
    width: 100%;
    background: #fff;
    border-radius: 15px;
}

.split-chunk {
    margin-bottom: 56rpx;
    width: 100%;
    height: 1px;
    background: rgba(233, 233, 233, 0.5);
}

/*我的订单css  */

.opt-lists {
