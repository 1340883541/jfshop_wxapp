page {
    height: 100%;
    min-height: 100%;
    background-color: #fff;
}

.w-wrap {
    position: relative;
    min-height: 100%;
    padding-bottom: 110rpx;
    background-color: #fff;
}
/* 占位 */
.order-place{
    height: 30rpx;
    background-color: #f1f2f6;
}
/* 地址 */
/* 没有地址信息时 */
.address-no {
    position: relative;
    height: 120rpx;
    line-height: 120rpx;
    padding: 0 30rpx;
    font-size: 30rpx;
    background-color: #fff;
    color: #333;
}

.address-no:after {
    content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    height: 1px;
    border-bottom: 1px solid #f1f1f1;
    color: #f1f1f1;
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}

.address-no .address-icon {
    width: 54rpx;
    height: 100%;
    background-position: left center;
    background-size: 32rpx 36rpx;
}

.address-no .right-icon {
    float: right;
    width: 10rpx;
    height: 100%;
    background-position: left center;
    background-size: 10rpx 18rpx;
}

/* 有地址信息时 */

.address-have {
    padding: 16rpx 30rpx 24rpx 10rpx;
    background-color: #fff;
}

.address-have > view {
    display: inline-block;
    vertical-align: middle;
}

.address-have .left {
    width: 70rpx;
    text-align: center;
}

.address-have .left .address-icon {
    display: block;
    width: 100%;
    height: 70rpx;
    background-position: center;
    background-size: 32rpx 36rpx;
}

.address-have .center {
    width: 590rpx;
}

.address-have .center .receiver {
    height: 60rpx;
    line-height: 60rpx;
    font-size: 30rpx;
    color: #333;
}

.address-have .center .receiver .txt0 {
    margin-right: 50rpx;
}

.address-have .center .receiver .txt1 {
    margin-right: 20rpx;
}

.address-have .center .receiver .txt2 {
    position: relative;
    display: inline-block;
    width: 50rpx;
    height: 30rpx;
    line-height: 30rpx;
    text-align: center;
    font-size: 20rpx;
    color: #666;
    vertical-align: middle;
}

.address-have .center .receiver .txt2:before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    width: 200%;
    height: 1px;
    border: 1px solid #999;
    color: #999;
    height: 200%;
    -webkit-transform-origin: left top;
    transform-origin: left top;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
}

.address-have .center .address {
    line-height: 36rpx;
    font-size: 26rpx;
    color: #999;
}

.address-have .right {
    width: 50rpx;
}

.address-have .right .right-icon {
    display: block;
    width: 100%;
    height: 70rpx;
    background-position: right center;
    background-size: 10rpx 18rpx;
}

/* 商品信息 */
/* 多个商品 */
.order-goods {
    position: relative;
    padding: 0 30rpx;
    background-color: #fff;
}

.order-goods::before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    border-top: 1px solid #f1f1f1;
    color: #f1f1f1;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}

.order-goods-info {
    padding-top: 32rpx;
    padding-bottom: 30rpx;
}

.order-goods-info swiper {
    width: 510rpx;
    height: 150rpx;
}

.order-goods-info swiper-item {
    box-sizing: border-box;
    width:170rpx;
    padding-right: 20rpx;
}
.order-goods-info image {
    width: 150rpx;
    height: 150rpx;
    margin-right: 26rpx;
}
.order-goods-info image.placeholder-icon{
    background-size:60% !important;

}

.order-goods-info image:last-child {
    margin: 0;
}

.order-goods-info .order-goods-num {
    height: 150rpx;
    line-height: 150rpx;
    font-size: 30rpx;
    color: #333;
}

.order-goods-info .right-icon1 {
    padding-left: 30rpx;
    height: 150rpx;
    background-position: right center;
    background-size: 10rpx 18rpx;
}

.order-msg {
    height: auto;
}

.order-msg .order-msg-cap {
    width: 130rpx;
    font-size: 28rpx;
    color: #333;
    line-height: 98rpx;
}

.order-msg .order-msg-textarea {
    width: 560rpx;
    padding: 16rpx 0;
}

.order-msg .order-msg-textarea.android-txt{
    padding:30rpx 0 16rpx;
}
.order-msg textarea {
    padding-left: 16rpx;
    width: 100%;
    font-size: 28rpx;
}
.order-msg .order-msg-textarea.android-txt textarea{
    padding-top: 8rpx; 
}
.order-msg textarea.hidden{
    display: none;
}
.order-msg textarea.show{
    display: block;
}

.order-msg-textarea-place {
    font-size: 28rpx;
    line-height: 48rpx;
    color: #ccc;
}

/* 单个商品样式 */
.goods-info {
    width: 100%;
    height: 210rpx;
    padding: 30rpx 0;
}

.goods-info .goods-left-img {
    width: 150rpx;
    height: 150rpx;
}

.goods-info .goods-left-img image {
    display: block;
    width: 100%;
    height: 100%;
    border-radius: 0;
}
.goods-info .goods-left-img image.placeholder-icon {
    background-size:60% !important;
}

.goods-info .goods-right-info {
    position: relative;
    width: 520rpx;
    padding-left: 20rpx;
}

.goods-info .goods-r-title {
    height: 72rpx;
    line-height: 36rpx;
    font-size: 28rpx;
    color: #333;
    margin-top: -8rpx;
    margin-bottom: 16rpx;
}

.goods-info .goods-r-desc {
    height: 26rpx;
    line-height: 26rpx;
    font-size: 26rpx;
    color: #666;
    margin-bottom: 20rpx;
}

.goods-info .goods-r-price {
    height: 28rpx;
    line-height: 28rpx;
    font-size: 28rpx;
    color: #ff220b;
}

.goods-info .goods-number {
    position: absolute;
    right: 0;
    bottom: 0;
    width: auto;
    font-size: 30rpx;
    line-height: 30rpx;
    color:#333;
}

/* 选择优惠券样式 */
.order-sale{
    background-color: #fff;
    padding: 0 30rpx;
}
.order-sale-list{
    position: relative;
    min-height: 100rpx;
}
.order-sale-list:before{
    content: "";
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    height: 1px;
    border-bottom: 1px solid #e9e9e9;
    color: #e9e9e9;
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}
.order-sale-list .left{
    line-height: 100rpx;
    font-size: 30rpx;
    color:#333333;
}
.order-sale-list .left text{
    margin-left: 26rpx;
}
.order-sale-list .left .jifen-icon{
    margin-top: 35rpx;
    width:30rpx;
    height: 30rpx;
    vertical-align: middle;
    background-position: center;
    background-size: cover;
    margin-left: 12rpx;
    /* top:-2rpx; */
}
.order-sale-list .no-use{
    font-size:28rpx;
    color:#666666;
    line-height: 100rpx;
}
.order-sale-list .use{
    color:#ff220b;
}
.order-sale-list .more-icon{
    padding-left: 30rpx;
    padding-bottom: 100rpx;
    vertical-align: middle;
    background-position: right center;
    background-size: 10rpx 18rpx;
}
.order-sale-list .switch-icon{
    display: block;
    width:72rpx;
    height: 44rpx;
    margin-top: 26rpx;
    margin-bottom: 34rpx;
    float: right;
}
.order-sale-list .switch-icon.yunshop-icon{
    float: left;
}
.order-sale-list .show-yunshop{
    position: relative;
    float: right;
    font-size: 24rpx;
    color:#333333;
    width:100rpx;
    line-height: 100rpx;
    padding-left: 24rpx;
    box-sizing: border-box;
}
.order-sale-list .show-yunshop .bottom-icon{
    position: absolute;
    right:0;top:50%;
    transform: translateY(-50%);
    border-top:10rpx solid #70726f;
    border-right: 9rpx solid transparent;
    border-left: 9rpx solid transparent;
}
/* 一元云购 */
.yunshop-place{
    width:100%;
    height: 20rpx;
    background:-webkit-gradient(linear, 0 0, 0 bottom, from(#f8f8f9), to(#fff)); 
}
.yunshop-goods{
    background-color: #fff;
    padding:0 30rpx;
}
.yunshop-goods .yunshop-goods-title{
    height: 88rpx;
    margin-bottom: 2rpx;
    line-height: 88rpx;
    font-size: 30rpx;
    color:#333333;
    text-align: center;
}
.yunshop-goods .yunshop-goods-desc{
    line-height: 36rpx;
    font-size: 26rpx;
    color:#999999;
}
.yunshop-goods .yunshop-goods-lists{
    margin-top: 4rpx;
}
.yunshop-goods .yunshop-goods-list{
    margin-top: 30rpx;
}
.yunshop-goods-left{
    position: relative;
}
.yunshop-goods .yunshop-goods-list image{
    display: block;
    width:150rpx;
    height: 150rpx;
}
.yunshop-goods-list .yunshop-goods-left text{
    position: absolute;
    bottom:0;right:10rpx;
    color:#fff;
    font-size: 20rpx;
    height: 34rpx;
    line-height: 34rpx;
}
.yunshop-goods-list .yunshop-goods-right{
    position: relative;
    width:520rpx;
    height: 150rpx;
}
.yunshop-goods-list .yunshop-goods-right .yunshop-goods-rtop{
    max-height: 108rpx;
    font-size: 28rpx;line-height: 36rpx;
    margin-top: -6rpx;
    overflow: hidden;
}
.yunshop-goods-list .yunshop-goods-right .yunshop-goods-rbottom{
    position: absolute;
    left:0;
    bottom:0;
    font-size: 28rpx;
    height: 28rpx;
    line-height: 28rpx;
    color:#333333;
}
.yunshop-goods-list .yunshop-goods-right .yunshop-goods-rbottom text{
    color:#ff220b;
}

.yunshop-goods-packup{
    margin-top: 18rpx;
    height: 64rpx;
    line-height: 64rpx;
    text-align: center;
    font-size:  24rpx;
    color:#999999;
}
.yunshop-goods-packup .w-top-icon{
    position: relative;
    top:-4rpx;
    padding-left: 18rpx;
    padding-top: 10rpx;
    background-position: center;
    background-size: 18rpx 10rpx;
    margin-left: 10rpx;
}
/* 金额显示 */
.price-place{
    width:100%;
    height: 20rpx;
    background:-webkit-gradient(linear, 0 0, 0 bottom, from(#fff), to(#f8f8f9)); 
}
.order-prices{
    padding: 20rpx 30rpx 13rpx;
    background-color: #fff;
}
.order-prices .order-prices-list{
    height: 60rpx;
    line-height: 60rpx;
    font-size: 28rpx;
}
.jifen_youhui{
    color: #5a5b5b !important;
    font-size: 24rpx;
}
.order-prices .order-prices-list .left{
    color:#333333;
}
.order-prices .order-prices-list .right{
    color:#ff220b;
}
.order-prices .order-prices-list .pack-mail{
    color:#333333;
}
/* 底部按钮 */
.order-foot-info{
    position: fixed;
    left:0;bottom:0;
    width: 100%;
    height: 110rpx;
    line-height: 110rpx;
    color:#333333;
    background-color: #fff;
}
.order-foot-info:before{
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    border-top: 1px solid #f5f5f5;
    color: #f5f5f5;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}
.order-foot-info .order-foot-txt{
    font-size: 24rpx;
}
.order-foot-info .order-foot-txt text{
    font-size: 32rpx;
    color:#ff220b;
}
.order-foot-info .order-foot-btn{
    width:230rpx;
    height: 80rpx;
    margin: 10rpx 40rpx 0 18rpx;
    background-color: #000;
    font-size: 32rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 40rpx;
    color: #fff;
}
.order-foot-info .order-foot-gray{
    width:230rpx;
    height: 80rpx;
    margin: 10rpx 40rpx 0 18rpx;
    background-color: #ccc;
    font-size: 32rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 40rpx;
    color: #fff;
}
/* 云购商品放大 */
.yunshop-fd{
    position: fixed;
    z-index:-1;
    left:0;top: 0;
    width:100%;
    height: 100%;
}
.yunshop-fd.z-index-1001{
    z-index:1001;
    background-color: rgba(0,0,0,0.4);
}
.yunshop-fd .yunshop-fd-box{
    position: absolute;
    width:690rpx;
    height: 690rpx;
    left:50%;
    top:50%;
     transform: translate(-50%,-50%) scale(1);
}
.yunshop-fd .yunshop-fd-box swiper{
    width:100%;
    height: 100%;
}
.yunshop-fd .yunshop-fd-box image{
    display: block;
    width:100%;
    height: 100%;
}
.yunshop-fd .yunshop-fd-box text{
    position: absolute;
    right:20rpx;
    bottom:8rpx;
    color:#fff;
    height: 50rpx;
    font-size: 32rpx;
    line-height: 50rpx;
}