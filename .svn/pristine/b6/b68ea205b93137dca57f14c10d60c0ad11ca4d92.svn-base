/* pages/good/good.wxss */
.container{
    width: 100%;
    min-height: 100vh;
    background: #F7F7F7;
}

scroll-view{
    width: 100%;
    height: 100vh;
}

.top-bar{
    /* position: fixed; */
    top:0;
    left:0;
    z-index: 10;
    background: #000;
}

.hide-search{
    height: 0 !important;
}
.hide-display{
    display: none;
}
.fixed_box{
    position: fixed;
}

.search-component{
    width: 100%;
    height: 96rpx;
    overflow: hidden;
    padding-left: 30rpx;
    transition: height 0.2s;
    -webkit-transition: height 0.2s;
}

.search-component::after{
    content: '';
    display: block;
    clear: both;
}

.search-v{
    float: left;
    width: calc(100% - 150rpx);
    height: auto;
    margin-top: 12rpx;
    margin-bottom: 12rpx;
}
.search-v search{
  width:550rpx;
}
.search-float{
    width: 100%;
}

.search-icon-v{
    float: left;
    width: 100rpx;
    height: 72rpx;
    padding-top: 22rpx;
}

.search-icon-v image{
    display: block;
    width: 36rpx;
    height: 35rpx;
    margin: 0 auto;
}

.filter-nav-bar{
    height: 140rpx;
    background:url('data:image/jpeg;base64,')
no-repeat center center;
background-size: cover;
}

.filter-item{
    width:33.333%;
    float:left;
}

.filter-v{
    display: inline-block;
    padding: 30rpx 0;
    margin: 0 auto;
    /* border-bottom: 4rpx solid #ffffff; */
    border-bottom: 0;
}

.filter-text{
    font-size: 30rpx;
    color: #fff;
}

.filter-icon{
    display: inline-block;
    width: 16rpx;
    height: 52rpx;
    margin-left: 10rpx;
    background-size: 100% 100%;
    vertical-align: top;
}

.filter-icon icon{
    width: 0px;
    height: 0px;
    border-left: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
}
.filter-icon .icon1{
    position: relative;
    top: -10rpx;
    border-bottom: 10rpx solid #ccc;
}
.filter-icon .icon2{
    position: relative;
    top: -40rpx;
    border-top: 10rpx solid #ccc;
}
.filter-item-active1 .filter-icon .icon1{
    border-bottom-color:#333;
}
.filter-item-active2 .filter-icon .icon2{
    border-top-color:#333;
}
.filter-item:first-child .filter-icon{
    display: none;
}

.filter-item-active .filter-v {
    border-bottom: 4rpx solid #858585;  
}

.filter-item-active .filter-text {
    font-weight: bold;
}

/*列表css  */
.goods-lists{
    width: 100%;
    height: auto;
    background: #fff;
    /* padding-top: 686rpx; */
    padding-bottom: 30rpx;
}

.good-lists {
    padding-left: 20rpx;

    background: #fff;
}

.good-lists::after{
    content: '';
    display: block;
    clear: both;
}
.good-listsone{
    padding-top: 45rpx;
}

.good-item {
    float: left;
    width: 50%;
    height: auto;
    margin-bottom: 46rpx;
    padding-right: 20rpx;
    position: relative;
}
.good-item .good-square{
    border:1px solid #bebebe;
}
.good-pic-s {
    width: 100%;
    height: 346rpx;
    overflow: hidden;
    background: #fff;
    /* border-radius: 16rpx; */
    position: relative;
}
.good-pic-s.placeholder-icon{
    background-size: 52% !important;
}

.good-pic-s image {
    display: block;
    width: 100%;
    height: 100%;
}

.good-pic-s .good-vol{
    width: 100%;
    height: auto;
    padding: 20rpx;
    color: #ffffff;
    font-size: 24rpx;
    text-align: right;
    text-shadow: 0px 1px 2px rgba(0, 0, 0, 0.3);
    position: absolute;
    left: 0;
    bottom: 0;
    z-index: 3;
}

.good-title-s {
    display:-webkit-box;
    text-align:left;
    font-size:30rpx;
    color:#727171;
    line-height:44rpx;
    padding-top:18rpx;
    padding-bottom:9px;
    height:74rpx;
    padding-left: 10rpx;
    padding-right: 10rpx;
}

.good-price-s{
    width: 100%;
    height: auto;
    display: flex;
    display: -webkit-flex;
    margin-top: 22rpx;
    padding-left: 10rpx;
    padding-right: 10rpx;
}
.jifen_y{
    font-size: 24rpx;
    line-height: 44rpx;
    text-align: right;
    padding-left: 4px;
}
.good-price-s .que-btn{
    margin-top: 5rpx;
}
.good-price-s::after{
    content: '';
    display: block;
    clear: both;
}

.good-cur-price {
    color: #000;
    font-size: 32rpx;
    margin-right: 10rpx;
    flex: 1;
    font-weight:bold;
}

.good-ori-price {
    float: left;
    color: #999999;
    font-size: 24rpx;
    margin-top: 10rpx; 
    text-decoration: line-through;
}

/* 列表模式 */
.icon-box{
    width: 100%;
    height: auto;
    padding: 45rpx 20rpx 20rpx 20rpx;
    background: #ffffff;
}

.icon-list{
    width: 100%;
    height: auto;
    margin-top: 45rpx;
    background: #eee;
    padding: 20rpx;
    display: flex;
    display: -webkit-flex;
    position: relative;
}

.icon-list:first-child{
    margin-top: 0;
}

.icon-list::after{
    content: '';
    display: block;
    clear: both;
}
.list-box-img{
    display: block;
    width: 278rpx;
    height: 278rpx;
    overflow: hidden;
    background: #fff;
    /* border-radius: 8rpx; */
}
.list-box-img.placeholder-icon{
    background-size:60% !important;
}
.icon-img{
    width:100%;
    height: 100%;
    overflow: hidden;
    /* border-radius: 8rpx; */
}

.icon-detail{
    flex: 1;
    width: calc(100% - 240rpx);
    height: auto;
    padding-left: 20rpx;
    /* border-bottom: 1rpx solid #e5e5e5; */
}

.icon-detail-tle{
    width: 100%;
    height: 88rpx;
    color: #333333;
    font-size: 30rpx;
    line-height: 44rpx;
}

.icon-detail-price{
    width: 100%;
    height: auto;
    /* padding-top: 40rpx; */
    padding-top: 62rpx;
    /* padding-bottom: 28rpx; */
}

.icon-detail-price .icon-cur-price{
    color: #da1c09;
    font-size: 32rpx;
    text-align: left;
}
.jifen_y{
    color: #5d5d5d !important;
    font-size: 24rpx !important;
}
.icon-detail-price .icon-ori-price{
    color: #999999;
    font-size: 24rpx;
    text-decoration: line-through;
}

.icon-vol{
    width: 100%;
    height: auto;
    color: #999999;
    font-size: 24rpx;
    padding-top: 5rpx;
    padding-bottom: 12rpx;
}
