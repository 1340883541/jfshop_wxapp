page{
    height: 100%;
    min-height: 100%;
}
.w-wrap{
    background-color: #fff;
    padding-bottom: 30rpx;
}
.cur{
 height: 100vh !important;
 overflow: hidden !important;
}
.w-wrap image{
    
}
.info_body{
    padding: 0 30rpx;
}
.info_body image{
    width: 100% !important;
}
button::after {
    border: none;border-radius: 0;
}
.notice_topbox{
    display: flex;
}
.share_switch{
    /* position: fixed; */
    display: block;
    height: 50rpx;
    padding: 10rpx 20rpx;
    /* background: #4dd225; */
    background: rgba(26,208,183,0.2);
    /* z-index: 1;
    right: -20rpx;
    top: 38rpx; */
    border: none;
    border-radius: 50rpx;
    line-height: 50rpx;
    margin-top:30rpx;
    margin-right:-16rpx;
}
.share_switch image{
    width: 30rpx !important;
    height: 30rpx;
    float:left;
}
.share_switch text{
    font-size: 24rpx;
    color: #15bda6;
    position: relative;
    line-height:30rpx;
    float:left;
    padding-left:8rpx;padding-right:8rpx;
}
.notice-detail-title{
    height: 110rpx;
    line-height: 111rpx;
    overflow: hidden;
    font-size: 38rpx;
    color:#333333;
    font-weight: bold;
    flex: 1;
    padding-left: 30rpx;
}
.notice-detail-cap{
    font-size: 30rpx;
    color:#666666;
    padding-bottom: 10rpx;
    line-height: 60rpx;
}
.notice-detail-img{
    width:100%;
    height: 400rpx;
}
.notice-detail-img image{
    display: block;
    width:100%;
    height: 100%;
}
.notice-detail-txt{
    padding-top: 10rpx;
    font-size: 30rpx;
    color:#666666;
    line-height: 60rpx;
}

.share_selectbody{
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 2;
    top: 0;
    left: 0;
    background:rgba(0,0,0,0.2);
}
.share_select{
    background: #fff;
	position: absolute;
	width: 100%;
	bottom: 0;
    left: 0;
}
.share_selectop{
    height: 330rpx;
    padding-top: 70rpx;
    padding-bottom: 60rpx;
  	border-bottom: 1px solid #ebebeb;
}
.share_selectop .share_iconbox{
    width: 50%;
    float: left;
    border: none;
    border-radius: 0;
    background: transparent;
    padding: 0;
}
.share_selectop .share_iconbox image{
    width: 90rpx;
	height: 90rpx;
	display: block;
	margin: auto;
}
.share_selectop .share_iconbox .share_text{
	display: block;
	margin: auto;
    text-align: center;
    color: #999;
    font-size: 24rpx;
    line-height:30rpx;
    margin-top:20rpx;
}
.share_selecbot{
    width: 100%;
    height: 140rpx;
    text-align: center;
    line-height: 120rpx;
    color: #212121;
}
.canvas_body{
    display: block;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.6);
    position: fixed;
    bottom: 0;
    left: 0;
}
.canvas_box{
    width: 100%;
    height: 530px;
    background: #fff;
    display: block;
    position: absolute;
    bottom: 0;
    left: 0;
}

.can_top{
    text-align: center;
    color: #666;
    font-size: 33rpx;
    display: block;
    width:100%;
    height:70px;
    line-height:70px;
    position: relative;
}
/* .can_top text{
    flex: 1;
} */
.can_top image{
    width: 40rpx;
    height: 40rpx;
    display: block;
    color: #999;
    position: absolute;
    right: 50rpx;
    top: 46rpx;
}
.can_center{
    width: 100%;
    height: 332px;
    display: block;
    margin: auto;
    
}
.can_cbox{
    width: 230px;
    height: 100%;
    display: block;
    margin: auto;
    box-shadow: 0px 0px 10px #ececec;
}
.codebox{
    position: fixed;
    bottom: 0;
    left: 0;
    background-repeat:no-repeat; 
    background-size:cover;
}
.can_c{
    width: 100%;
    height: 332px;
    box-shadow: 0px 0px 10px #ededed;
    background: #fff;
}
.saveimg{
    width: 100%;
    padding: 70rpx 0 0 0;
}
.saveimg button{
    display: block;
    width: 340rpx;
    height: 90rpx;
    color: #fff;
    background: #0fcab7;
    line-height: 90rpx;
    text-align: center;
    font-weight: bold;
    font-size: 30rpx;
    margin: auto;
}
.saveimg text{
    display: block;
    width: 100%;
    text-align: center;
    line-height: 50rpx;
    font-size: 24rpx;
    color: #656565;
}
.codeboxcanvars{
    width: 100%;
    height: 500px;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 1;
    background-repeat:no-repeat; 
    background-size:cover;
}
@media screen and (max-width: 350px) {
    .canvas_box{
        height: 490px;
    }
    .saveimg {
        padding:50rpx 0 0 0;
    }
}